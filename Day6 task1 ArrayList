import java.util.ArrayList;
class ArrayListsProgram {
    public static void main(String[] args) {
        ArrayList<Object> arr=new ArrayList<Object>();
        arr.add("Harish");
        arr.add("A");
        arr.add("CS");
        arr.add("III year");
        arr.add(0, "Hari");
        arr.set(2, "Sudha");
        arr.remove(1);
 System.out.println(arr.get(3));
        for(Object data:arr){
        System.out.println(data);
        }   
    
    }
}
