public class laptop{
 String model;
 String ram;
  laptop(String model,String ram){
   this.model=model;
   this.ram=ram;
    System.out.println("laptop"+ "" +model+""+ram);
  }
    laptop(laptop copy){
        this.model=copy.model;
        this.ram=copy.ram;
    }

 void Display(){
  System.out.println("laptop"+ "" +model+""+ram);
 
     
 }
 public static void main(String[]args){
      laptop ref=new laptop("APPLE" ,"SO-DIM");
      laptop ref1=new laptop(ref);
System.out.println("Copy constructor");
ref.Display();
ref1.Display();
}

     
 
 
//copy constructor

}



   


    
